                    $("#OptionRegister").fadeOut();
                    $("#OtpLogin").fadeIn();
                }
                else {
                    alert("Email or Mobile Already exist.");
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
        });
    });

